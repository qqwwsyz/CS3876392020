<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="wrap_content"
android:layout_height="match_parent"
android:background="#82B1FF"
android:gravity="center"
android:orientation="vertical"
android:visibility="visible"
tools:context=".Menu"
tools:visibility="visible">

<ImageView
    android:id="@+id/imageView7"
    android:layout_width="wrap_content"
    android:layout_height="75dp"

    android:layout_marginStart="14dp"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="14dp"
    android:layout_marginEnd="10dp"
    android:background="#00FFFFFF"
    app:srcCompat="@drawable/go" />

    <com.google.android.material.card.MaterialCardView
        android:layout_width="354dp"
        android:layout_height="196dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="20dp"
        app:contentPadding="5dp">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="290dp"
            android:layout_height="173dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/sb" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="162dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/averia_serif_libre_bold_italic"
            android:text="Keep it!"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="#1D0747"
            android:textSize="36sp" />

        <Button
            android:id="@+id/button1"
            android:layout_width="100dp"
            android:layout_height="35dp"
            android:layout_marginStart="230dp"
            android:layout_marginTop="111dp"
            android:background="#FF80AB"
            android:onClick="gotoBelly"
            android:text="Start"
            android:textAlignment="center" />
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card2"
        android:layout_width="355dp"
        android:layout_height="188dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="20dp"
        app:contentPadding="10dp">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="284dp"
            android:layout_height="203dp"
            app:srcCompat="@drawable/haha" />

        <TextView
            android:id="@+id/button3"
            android:layout_width="190dp"
            android:layout_height="84dp"
            android:layout_marginStart="200dp"
            android:layout_marginTop="10dp"
            android:background="#2DFFFFFF"
            android:fontFamily="@font/averia_serif_libre_bold_italic"
            android:text="Abdominal lacerator"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="#3B206A"
            android:textSize="30sp"
            app:backgroundTint="#00917C94" />

        <Button
            android:id="@+id/button2"
            android:layout_width="121dp"
            android:layout_height="38dp"
            android:layout_below="@+id/imageView4"
            android:layout_marginStart="230dp"
            android:layout_marginTop="111dp"
            android:background="#FF80AB"
            android:onClick="gotoPacks"
            android:text="Start"
            android:textAlignment="center" />
    </com.google.android.material.card.MaterialCardView>


</LinearLayout>
